{
  "name": "gatsby-plugin-intl",
  "description": "Gatsby plugin to add react-intl onto a site",
  "version": "0.3.7",
  "author": "Daewoong Moon <wiziple@gmail.com>",
  "bugs": {
    "url": "https://github.com/titeta-com/gatsby-plugin-intl"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@formatjs/intl-pluralrules": "^4.1.6",
    "@formatjs/intl-relativetimeformat": "^9.3.3",
    "browser-lang": "^0.1.0",
    "intl": "^1.2.5",
    "react-intl": "^5.23.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "babel-preset-gatsby-package": "^2.3.0",
    "cross-env": "^7.0.3",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "homepage": "https://github.com/titeta-com/gatsby-plugin-intl",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-intl",
    "i18n",
    "intl",
    "react-intl"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "4.x"
  },
  "repository": "https://github.com/titeta-com/gatsby-plugin-intl",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write {src,__tests__}/**/*.{js,jsx}",
    "test": "jest",
    "clean": "rimraf *.js"
  }
}
